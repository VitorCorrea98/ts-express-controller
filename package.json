{
	"name": "ts-express-generic",
	"version": "0.0.7",
	"files": [
		"dist"
	],
	"description": "A generic TypeScript controller and service response types for Express",
	"keywords": [
		"typescript",
		"express",
		"API",
		"controller",
		"generic"
	],
	"author": "Vitor Correa",
	"license": "MIT",
	"type": "commonjs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "jest",
		"check": "npx biome check",
		"check:fix": "npx biome check --write",
		"prepublishOnly": "npm run check:fix && npm run build"
	},
	"dependencies": {
		"express": "^5.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@tsconfig/node-ts": "^23.6.1",
		"@types/express": "^5.0.1",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.15.17",
		"jest": "^29.7.0",
		"ts-jest": "^29.3.2",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3"
	}
}
